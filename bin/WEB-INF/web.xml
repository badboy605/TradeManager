<web-app>
	<icon>
		<small-icon>/images/trade.gif</small-icon>
		<large-icon>/images/trade.gif</large-icon>
	</icon>
	<display-name>Trade-DisplayName (Trade)</display-name>
	<description>Description body (Trade)</description>
	<servlet>
