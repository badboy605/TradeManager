			<small-icon>/icons/trade.gif</small-icon>
			<large-icon>/icons/trade.gif</large-icon>
		</icon>
		<servlet-name>JnlpDownloadServlet</servlet-name>
		<display-name>Trade Manager Tool</display-name>
		<description>The down load servlet</description>
		<servlet-class>com.sun.javaws.servlet.JnlpDownloadServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>JnlpDownloadServlet</servlet-name>
		<url-pattern>/app/*</url-pattern>
	</servlet-mapping>
</web-app>
